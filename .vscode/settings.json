{
  "editor.mouseWheelZoom": true,
  "workbench.startupEditor": "none",
  "debug.terminal.clearBeforeReusing": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "blade": "html"
  },
  "code-runner.runInTerminal": true,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.suggestSelection": "first",
  "code-runner.clearPreviousOutput": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro Darker",
  "liveServer.settings.donotShowInfoMsg": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "'Fira Code', 'FiraCode Nerd Font', Consolas, Courier New, monospace",
  "editor.fontLigatures": "true",
  "editor.fontSize": 16,
  "liveServer.settings.donotVerifyTags": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs": true,
  "editor.wordWrap": "on",
  "[php]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "git.autofetch": true,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "files.autoSave": "afterDelay",
  "git.openRepositoryInParentFolders": "never",
  "terminal.integrated.env.windows": {},
  "console-ninja.toolsToEnableSupportAutomaticallyFor": {
    "live-server-extension": true,
    "live-preview-extension": true
  },
  "codeium.enableConfig": {
    "*": true,
    "properties": true,
    "xml": true,
    "prisma": true,
    "ignore": true,
    "plaintext": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "indentRainbow.updateDelay": 100,
  "indentRainbow.includedLanguages": [],
  "indentRainbow.colors": [
    "rgba(255,255,64,0.7)",
    "rgba(127,255,127,0.7)",
    "rgba(255,127,255,0.7)",
    "rgba(79,236,236,0.7)"
  ],
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "indentRainbow.errorColor": "rgba(255,255,255,0)",
  "console-ninja.featureSet": "Community",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "branch-create:command"
  ],
  "git.enableSmartCommit": true,
  "terminal.integrated.cursorBlinking": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "powermode.presets": "",
  "powermode.shake.enabled": false,
  "powermode.combo.counterEnabled": "hide",
  "powermode.combo.location": "off",
  "powermode.enabled": true,
  "WillLuke.nextjs.addTypesOnSave": true,
  "WillLuke.nextjs.hasPrompted": true,
  "window.zoomLevel": -1,
  "terminal.integrated.enableImages": true,
  "console-ninja.showWhatsNew": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.mouseWheelZoom": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.fontFamily": "'Fira Code', 'FiraCode Nerd Font', Consolas, Courier New, monospace",
  "editor.stickyScroll.enabled": false,
  "terminal.integrated.enableFileLinks": "on",
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "liveServer.settings.port": 5500,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "explorer.confirmDragAndDrop": false,
  "tailwindCSS.emmetCompletions": true,
  "php.validate.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "css"
  ],
  "intelephense.files.associations": ["*.php", "*.phtml", "*.module", "*.inc"],
  "blade.format.enable": true,
  "blade.format.wrapLineLength": 120,
  "blade.format.wrapAttributes": "auto",
  "path-autocomplete.extensionOnImport": true,
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "python.terminal.focusAfterLaunch": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "codesnap.shutterAction": "copy",
  "codesnap.target": "container",
  "update.showReleaseNotes": false,
  "console-ninja.predictiveLoggingEnabled": false,
  "console-ninja.showWelcomeMessageInTools": false,
  "console-ninja.captureFunctions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "pieces.cloudCapabilities": "Blended",
  "pieces.telemetry": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#f52704",
    "activityBar.background": "#f52704",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#04dc23",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#f52704",
    "statusBar.background": "#c31f03",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#f52704",
    "statusBarItem.remoteBackground": "#c31f03",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#c31f03",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#c31f0399",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#c31f03"
}
